{
      "rewrites": [
            {
                  "source": "/((?!api|_next|.*\\.).*)",
                  "destination": "/index.html"
            }
      ],
      "headers": [
            {
                  "source": "/site.webmanifest",
                  "headers": [
                        {
                              "key": "Content-Type",
                              "value": "application/manifest+json"
                        }
                  ]
            }
      ]
}